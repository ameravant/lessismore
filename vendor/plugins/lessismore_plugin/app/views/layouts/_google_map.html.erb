<%- content_for :head do -%>
<script type="text/javascript" charset="utf-8" src="http://maps.google.com/maps?file=api&amp;v=2&amp;key=<%= $GOOGLE_MAPS_API_KEY %>&sensor=true"></script>
<script type="text/javascript" charset="utf-8">
  function initialize() {
    if (GBrowserIsCompatible()) {
      var map = new GMap2($("map_canvas"));
      var latlon = new GLatLng(<%= lat %>, <%= lng %>)
      map.setCenter(latlon, <%= zoom %>);
      map.setMapType(G_PHYSICAL_MAP);
      map.setUIToDefault();
      map.addOverlay(new GMarker(latlon));
    }
  }
</script>
<%- end -%>

<%- content_for :onload do -%>
initialize()
<%- end -%>

<%- content_for :onunload do -%>
GUnload()
<%- end -%>
